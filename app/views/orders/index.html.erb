<div class="row">
	<div class="small-11 small-centered columns">

		<div class="row">
			<div class="small-12 columns">

				<div id="dashboard-marketplace-header">
					<%= image_tag "logo_header_marketplace.svg" %>
				</div>

				<div class="row">
					<div class="small-12 columns">
