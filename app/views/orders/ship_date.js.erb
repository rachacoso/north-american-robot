<% if @complete %>
	$('.overlay#order-item-overlay').fadeOut();
	$('#order-item').fadeOut();
	$('#ship-cancel-add').html("<%= j (render partial: 'ship_cancel_add', locals: {order: @order}) %>");
<% else %>
	$('#order-item-data').html("<%= j (render partial: 'ship_date', locals: {order: @order}) %>");
	$('.overlay#order-item-overlay').fadeIn();
	$('#order-item').fadeIn();
<% end %>
$('#order-display').html("<%= j (render partial: 'shared/orders/show') %>");
$('#ajax-success-data').html("Shipping Date Updated");
$('#ajax-message').fadeIn().delay( 1500 ).fadeOut();
initialize ();
initializeOrderControls();