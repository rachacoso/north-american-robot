<% unless profile.website.blank? %>
	<%= link_to "Website", "//#{clean_url(profile.website)}", target: "blank", class: "profile-social-link" %>
<% end %>

<% if !profile.website.blank? && !profile.facebook.blank? %>/<% end %>

<% unless profile.facebook.blank? %>
	<%= link_to "Facebook", "//#{clean_url(profile.facebook)}", target: "blank", class: "profile-social-link" %>
<% end %>

<% if (!profile.website.blank? || !profile.facebook.blank?) && !profile.linkedin.blank? %>/<% end %>

<% unless profile.linkedin.blank? %>
	<%= link_to "LinkedIn", "//#{clean_url(profile.linkedin)}", target: "blank", class: "profile-social-link" %>
<% end %>

<% if (!profile.website.blank? || !profile.facebook.blank? || !profile.linkedin.blank?) && !profile.twitter.blank? %>/<% end %>

<% unless profile.twitter.blank? %>
	<%= link_to "Twitter", "//#{clean_url(profile.twitter)}", target: "blank", class: "profile-social-link" %>
<% end %>

<% if (!profile.website.blank? || !profile.facebook.blank? || !profile.linkedin.blank? || !profile.linkedin.blank?) && !profile.instagram.blank? %>/<% end %>

<% unless profile.instagram.blank? %>
	<%= link_to "Instagram", "//#{clean_url(profile.instagram)}", target: "blank", class: "profile-social-link" %>
<% end %>