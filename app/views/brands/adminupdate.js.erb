<% if @brand.errors.any? %>
	<%= render partial: 'shared/ajax_message_error', locals: { updated_item: "Sorry please try again" } %>
<% else %>
	<%= render partial: 'shared/ajax_message', locals: { updated_item: "Brand Admin Settings" } %>
<% end %>
<% if @subscription_set %>
	$('#subscription-status').html("<%= j (render partial: 'admin/subscription_indicator', locals: {brand: @brand}) %>")
<% end %>