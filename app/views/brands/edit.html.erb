<%= render partial: "shared/topbar" %>
<%= render partial: "newuser", locals: { content: "newbrand_content" } if flash[:newuser] %>
<%= render partial: "newuser", locals: { content: "newaccount_content" } if flash[:newaccount] %>

<div class="row">
	<div class="small-12 columns marketplace-subpage-header">
		<div class="medium-6 columns" id="your-profile-header">
			<h1>YOUR PROFILE</h1>
			<h3>Preview &amp; Edit your marketplace profile.</h3>			
		</div><!--/.medium-6-->		
		<div class="medium-6 columns">
			<div class="row">
				<div class="small-6 columns">
					<h2>Edit Profile</h2>
					<small><span data-tooltip aria-haspopup="true" class="has-tip radius" title="Complete your brand profile so your potential  partners can learn about you and be better matched to you and your customer base.">What is this?</span></small>
				</div>
				<div class="small-6 columns">
				</div>		
			</div>
		</div><!--/.medium-6-->
	</div>
</div>

<div class="row collapse">
