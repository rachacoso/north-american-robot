<% partial_info = onboard_brand_partial(stage: stage) %>
<div class="row">
	<div class="small-12 columns text-center">
		<%= image_tag partial_info[:image_path] %>
	</div><!--/.small-12-->
</div><!--/.row-->
<div class="row">
	<div class="small-12 columns text-center">
		<%= render partial: "progress_brand", locals: {stage: stage} %>
	</div><!--/.small-12-->
</div><!--/.row-->
<hr>
<% partial_info[:file].each_with_index do |file,index| %>
	<% if partial_info[:locals] && partial_info[:locals][index.to_i].present? %>
		<%= render partial: partial_info[:file][index.to_i], locals: partial_info[:locals][index.to_i] %>
	<% else %>
		<%= render partial: partial_info[:file][index.to_i] %>
	<% end %>
	<br>
<% end %>
<br>