<% partial_info = onboard_brand_partial(stage: stage) %>
<div class="row">
	<div class="small-12 columns text-center">
		<%= image_tag partial_info[:image_path] %>
	</div><!--/.small-12-->
</div><!--/.row-->
<div class="row">
	<div class="small-12 columns text-center">
		<%= render partial: "progress_brand", locals: {stage: stage} %>
	</div><!--/.small-12-->
</div><!--/.row-->

<hr>
<% if partial_info[:locals] %>
	<%= render partial: partial_info[:file], locals: partial_info[:locals] %>
<% else %>
	<%= render partial: partial_info[:file] %>
<% end %>