<% if @brand %>
<%= render partial: 'brand', locals: {stage: get_onboard_stage(company:@brand, user:@current_user) } %>
<% elsif @retailer %>
<%= render partial: 'retailer', locals: {stage: get_onboard_stage(company:@retailer, user:@current_user) } %>
<% elsif @distributor %>
<%= render partial: 'distributor', locals: {stage: get_onboard_stage(company:@distributor, user:@current_user) } %>
<% end %>