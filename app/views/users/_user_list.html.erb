		
		<table class="contacts-table">
			<thead>
				<tr>
					<th>USERNAME</th>
					<th>First Name</th>
					<th>Last Name</th>
					<th>Title</th>
					<th>Phone</th>
				</tr>
			</thead>
			<tbody>
				<% b_or_d.users.each do |user| %>
				<tr>
					<td>
						<%= user.email.blank? ? "(no email)" : "#{user.email}".html_safe %>
						<% if @current_user.administrator? %>
						<%= link_to 'edit', edit_user_path(user), class: "itemformlink" %>
						<% if b_or_d.users.count > 1 %>
						<%= link_to 'delete', admin_delete_user_path(user), :method => 'delete', data: { :confirm => 'Are you sure you want to delete this user?'}, class: "itemformlink"  %>
						<% end %>
						<% end %>
					</td>
					<td>
						<%= user.contact.firstname.blank? ? "(nameless)" : "#{user.contact.firstname}".html_safe %>
					</td>
					<td>
						<%= user.contact.lastname.blank? ? "(nameless)" : "#{user.contact.lastname}".html_safe %>
					</td>
					<td>
						<%= user.contact.title.blank? ? "(no title)" : "#{user.contact.title}".html_safe %>
					</td>
					<td>
						<%= user.contact.phone.blank? ? "(no phone)" : "#{user.contact.phone.phony_formatted(format: :international, spaces: '-')}<br>".html_safe %>
					</td>
				</tr>
				<% end %>
			</tbody>
		</table>
