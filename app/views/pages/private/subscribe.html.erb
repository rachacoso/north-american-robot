<%= render partial: 'shared/v2/header/main' %>
<%= render partial: 'shared/v2/header/marketplace' %>

<div style="height: 100vh;">
<br><br><br>
	<div class="row">
		<div class="medium-10 medium-centered columns">
			<div class="row">
				<div class="small-12 columns text-center">
					<div class="new-user-subscription">
						<div id="notice">THIS AREA REQUIRES A SUBSCRIPTION</div><!--/#notice -->
					</div><!--/.new-user-subscription -->
				</div><!--/.small-12-->
			</div><!--/.row-->
			<br>
			<div class="row" data-equalizer>
				<div class="medium-4 columns">
					<div class="new-user-subscription" data-equalizer-watch>
						<div id="main">
							<div class="row">
								<div class="small-10 small-centered columns">
									<h1 class="greyed">STEP 1:</h1>
									<h1 class="greyed">APPROVAL</h1>
									<br>
									<h1>STEP 2:</h1>
									<h1>PAYMENT</h1>
								</div><!--/.small-12-->
							</div><!--/.row-->
						</div><!--/#main -->	
					</div><!--/.new-user-subscription -->
				</div><!--/.medium-4-->
				<div class="medium-8 columns">
					<div class="new-user-subscription"  data-equalizer-watch>
						<div id="main">
							<div class="row">
								<div class="small-10 small-centered columns">
									<h1>Hello Again!</h1>
									<h2>You've been approved and your subscription is ready to be activated!</h2>
									<%= link_to "PAY NOW", brand_subscription_url, class: "landing-button", style: "font-weight: 400; text-decoration: none; color: white;", remote: true %>
									<br><br>
									<h3> Please email us at <a mailto="info@landingintl.com" style="color:black;">info@landingintl.com</a> with any questions.</h3>
								</div><!--/.small-12-->
							</div><!--/.row-->
						</div><!--/#main -->
					</div><!--/.new-user-subscription -->
				</div><!--/.medium-8-->
			</div><!--/.row-->
		</div><!--/.small-12-->
	</div><!--/.row-->
</div><!--/.100vh-->
<%= render partial: 'shared/v2/footer/main' %>