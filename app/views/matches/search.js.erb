<% if @current_user.brand %>
	$('#match-list').hide().html("<%= j (render partial: 'brand_match_list', locals:{matches:@matches}) %>").prepend("You searched for '<%= @query %>'").fadeIn(500);
<% else %>
	$('#match-list').hide().html("<%= j (render partial: 'distributor_match_list', locals:{matches:@matches}) %>").prepend("You searched for '<%= @query %>'").fadeIn(500);
<% end %>
$(document).foundation();
$(document).foundation('tooltip', 'reflow');