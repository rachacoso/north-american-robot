	

		<% if i_contacted(match) %>
			<% if match.accepted %>
				<div class="conversation-action-tile <%= match.accepted? ? '' : 'active' %> text-center">
				<span data-tooltip aria-haspopup="true" class="has-tip radius" title="YOU CONTACTED THEM<BR> <SMALL> CONTACTED ON <%= match.c_at.strftime("%d %B %Y").upcase %></SMALL>">
					<%= image_tag "match_profile_icons_conversation_outgoing.svg" %></span><br>
					<% if !match.messages.blank? %>
						<h4>You started a conversation with <strong><%= profile.company_name %></strong></h4>
					<% else %>
						<h4><strong><%= profile.company_name %></strong> accepted your request and should be messaging you shortly.</h4>
					<% end %>
				</div>
				<% if !match.messages.blank? %>
					<div class="conversation-action-tile active text-center">
						<h4>Stuff for proceeding</h4>
						<%= button_to 'PROCEED TO PROPOSE', match_stage_path(match,'propose'), :class => "button tiny alert", remote: true %>
					</div>
				<% end %>
			<% else %>
				<div class="conversation-action-tile <%= match.accepted? ? '' : 'active' %> text-center">
					<h4><span data-tooltip aria-haspopup="true" class="has-tip radius" title="AWAITING THEIR RESPONSE<BR> <SMALL> CONTACTED ON <%= match.c_at.strftime("%d %B %Y").upcase %></SMALL>">
					<%= image_tag "match_profile_icons_waiting.svg" %></span><br>You have requested a conversation with <strong><%= profile.company_name %></strong> and are awaiting a response</h4>
				</div>
			<% end %> 								
		<% else %>
			
			<% if match.accepted %>
				<div class="conversation-action-tile <%= match.accepted? ? '' : 'active' %> text-center">
				<span data-tooltip aria-haspopup="true" class="has-tip radius" title="CONTACTED YOU<BR> <SMALL> CONTACTED ON <%= match.c_at.strftime("%d %B %Y").upcase %></SMALL>"><%= image_tag "match_profile_icons_conversation_incoming.svg" %></span><br>
					<h4><strong><%= profile.company_name %></strong> started a conversation with you</h4>
				</div>
				<% if !match.messages.blank? %>
				<div class="conversation-action-tile active text-center">
					<h4>Stuff for proceeding</h4>
					<%= button_to 'PROCEED TO PROPOSE', match_stage_path(match,'propose'), :class => "button tiny alert", remote: true %>
				</div>
				<% end %>		
			<% else %>
				<div class="conversation-action-tile <%= match.accepted? ? '' : 'active' %> text-center">
					<h4><strong><%= profile.company_name %></strong> would like to have a conversation with you</h4>
					<%= button_to 'ACCEPT', accept_match_path(match), :id => 'contact-accept', :class => "button tiny alert", remote: true %>
				</div>
			<% end %>


		<% end %>

	</div>